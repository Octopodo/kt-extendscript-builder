{
    "name": "kt-extendscript-builder",
    "version": "1.5.7",
    "description": "Vite based builder for transpile TypeScript to ExtendScript",
    "keywords": [
        "TypeScript",
        "ExtendScript",
        "Transpile"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "kt-build": "./dist/bin/kt-build.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "@babel/core": "^7.22.0",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-classes": "^7.22.0",
        "@babel/preset-env": "^7.22.0",
        "@babel/preset-typescript": "^7.22.0",
        "@rollup/plugin-babel": "^6.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.1",
        "types-for-adobe": "^7.2.2",
        "vite-cep-plugin": "^1.1.0",
        "yargs": "^17.7.2",
        "@rollup/plugin-terser": "^0.4.4"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^18.0.0",
        "@types/yargs": "^17.0.24",
        "@vitest/coverage-v8": "^3.0.9",
        "@vitest/ui": "^3.0.9",
        "fs-extra": "^11.3.0",
        "kt-core": "^1.0.6",
        "kt-testing-suite-core": "^1.6.1",
        "mock-fs": "^5.5.0",
        "typescript": "^5.0.0",
        "vitest": "^3.0.9"
    },
    "peerDependencies": {
        "typescript": ">=4.0.0",
        "vite": ">=4.0.0"
    }
}
